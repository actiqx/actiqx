<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.Actiqx" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Actiqx</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <engine name="android" spec="^6.2.3" />
    <plugin name="cordova-plugin-datepicker" spec="git+https://github.com/VitaliiBlagodir/cordova-plugin-datepicker.git" />
    <plugin name="phonegap-plugin-push" spec="^1.10.5">
        <variable name="SENDER_ID" value="125954626318" />
    </plugin>
    <plugin name="cordova-plugin-x-toast" spec="https://github.com/EddyVerbruggen/Toast-PhoneGap-Plugin.git" />
</widget>
